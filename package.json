{
    "name": "is-biz-mail",
    "version": "0.2.10",
    "license": "MIT",
    "description": "isBizMail tells you whether a given email address is free (gmail.com, yahoo.es, yandex.ru etc) or not",
    "homepage": "https://github.com/salaros/is-biz-mail",
    "devDependencies": {
        "coveralls": "^3.0.1",
        "mocha": "^5.1.1",
        "nyc": "^12.0.2",
        "should": "^13.2.1"
    },
    "main": "src/commonjs",
    "scripts": {
        "test": "./node_modules/.bin/nyc --reporter=html --reporter=text ./node_modules/mocha/bin/mocha",
        "coverage": "./node_modules/.bin/nyc report --reporter=text-lcov | ./node_modules/.bin/coveralls"
    },
    "keywords": [
        "email",
        "checker",
        "address",
        "free",
        "domain",
        "business",
        "business-email",
        "blacklist",
        "mailbox",
        "mail",
        "javascript",
        "javascript-library",
        "js",
        "es6",
        "commonjs",
        "commonjs-modules",
        "vanilla-javascript",
        "vanilla-js",
        "vanillajs"
    ]
}
